<script>
(function() {



  function init() {
    if (typeof window._initPage == 'function') {
      window._initPage();
      window._initPage = null;
    }
  }

  function afterImports() {

    if (!IOWA.Elements.Template.pages["onsite"]) {
      IOWA.Elements.Template.pages["onsite"] = {};
    }
    IOWA.Elements.Template.pages["onsite"].title = "Attend Onsite";

    init();
  }

  if (IOWA.Util.supportsHTMLImports) {
    afterImports();
  } else {
    document.addEventListener('polymer-ready', afterImports);
  }

})();
</script>

